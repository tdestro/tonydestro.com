package com.healthysight.impairments{		import flash.display.Bitmap;	import flash.display.BitmapData;		public class DistortionBitmap extends Bitmap{				public var vx:Number;		public var vy:Number;				public function DistortionBitmap(bitmapData:BitmapData = null, pixelSnapping:String = "auto", smoothing:Boolean = false){			vx = vy = 0;			super(bitmapData, pixelSnapping, smoothing);		}			}	}			
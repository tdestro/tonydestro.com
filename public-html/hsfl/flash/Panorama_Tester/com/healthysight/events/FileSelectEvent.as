package com.healthysight.events{		import flash.events.Event;		public class FileSelectEvent extends Event{						//- PRIVATE & PROTECTED VARIABLES -------------------------------------------------------------------------		//- PUBLIC & INTERNAL VARIABLES ---------------------------------------------------------------------------		public static const DEFAULT_NAME:String = "com.healthysight.events.FileSelectEvent";		// event constants		public static const SELECT:String = "select";		public var params:Object;						//- CONSTRUCTOR -------------------------------------------------------------------------------------------		public function FileSelectEvent($type:String, $params:Object, $bubbles:Boolean = false, $cancelable:Boolean = false)		{			super($type, $bubbles, $cancelable);			this.params = $params;		}				//- PRIVATE & PROTECTED METHODS ---------------------------------------------------------------------------		//- PUBLIC & INTERNAL METHODS -----------------------------------------------------------------------------		//- EVENT HANDLERS ----------------------------------------------------------------------------------------		//- GETTERS & SETTERS -------------------------------------------------------------------------------------		//- HELPERS -----------------------------------------------------------------------------------------------		public override function clone():Event		{			return new FileSelectEvent(type, this.params, bubbles, cancelable);		}		public override function toString():String		{			return formatToString("CustomEvent", "params", "type", "bubbles", "cancelable");		}				//- END CLASS ---------------------------------------------------------------------------------------------					}		}